{
  "name": "geostyler-cli",
  "version": "3.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "geostyler": "dist/src/index.js",
    "geostyler-cli": "dist/src/index.js"
  },
  "scripts": {
    "build": "npm run build-clean && npm run build-only",
    "build-clean": "rimraf build/ dist/",
    "build-only": "tsc -p tsconfig.json",
    "lint:build": "npm run lint && npm run build",
    "start": "node dist/src/index.js",
    "lint": "eslint . && tsc --noEmit --project tsconfig.json",
    "test": "node test.js",
    "build-binaries": "npm run build && pkg package.json",
    "package-binaries": "node package-binaries.js"    
  },
  "pkg": {
    "targets": ["node18-linux-x64", "node18-macos-x64", "node18-win-x64"],
    "outputPath": "./binaries"
  },  
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geostyler/geostyler-cli.git"
  },
  "keywords": [
    "geo",
    "styler",
    "geostyler",
    "cli"
  ],
  "author": "",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/geostyler/geostyler-cli/issues"
  },
  "homepage": "https://github.com/geostyler/geostyler-cli#readme",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@terrestris/eslint-config-typescript": "^3.1.0",
    "@types/gradient-string": "^1.1.2",
    "@types/node": "^18.16.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "np": "^7.7.0",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "geostyler-mapbox-parser": "^3.1.1",
    "geostyler-mapfile-parser": "^3.0.0",
    "geostyler-openlayers-parser": "^4.1.2",
    "geostyler-qgis-parser": "^2.0.0",
    "geostyler-sld-parser": "^5.0.1",
    "geostyler-style": "^7.2.0",
    "gradient-string": "^2.0.2",
    "minimist": "^1.2.8",
    "ol": "^7.3.0",
    "ora": "5.4.1",
    "terminal-image": "^2.0.0"
  }
}
