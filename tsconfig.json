{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "ES2022",
    "moduleResolution": "Node",
    "target": "ES2022",
    "outDir": "./dist",
    "lib": [
      "ES2022"
    ],
    "rootDir": ".",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  }
}
